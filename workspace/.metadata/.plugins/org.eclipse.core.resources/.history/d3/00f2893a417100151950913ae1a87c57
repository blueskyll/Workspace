class Family implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private People p;
	private int id;
	public Family(People p, int id){
		this.id = id;
		this.p = p;
	}
	
}

class People implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String name;
	public People(String name){
		this.name = name;
	}
}
